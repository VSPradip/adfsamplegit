{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "svdnordenor"
		},
		"AzureCosmosDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureCosmosDB'"
		},
		"AzureCosmosSink_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureCosmosSink'"
		},
		"AzureSQLOrder_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSQLOrder'"
		},
		"AzureSqlDbSales_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDbSales'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureCosmosDB_connectionString": {
						"value": "[parameters('AzureCosmosDB_connectionString')]"
					},
					"AzureCosmosSink_connectionString": {
						"value": "[parameters('AzureCosmosSink_connectionString')]"
					},
					"AzureSQLOrder_connectionString": {
						"value": "[parameters('AzureSQLOrder_connectionString')]"
					},
					"AzureSqlDbSales_connectionString": {
						"value": "[parameters('AzureSqlDbSales_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}